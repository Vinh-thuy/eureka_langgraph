from typing import TypedDict, List, Optional


class AgentState(TypedDict):
    """
    Canonical LangGraph Agent State.
    Version: 1.0
    """

    user_input: str
    route: Optional[str]
    result: Optional[str]
    intermediate_steps: List[str]

